# Environment variables for Label Studio with RTMLib ML Backend
# Copy this file to .env and adjust values as needed

# Label Studio API Configuration
LABEL_STUDIO_API_KEY=your_api_key_here

# ML Backend Configuration
DEVICE=cpu                    # cpu, cuda, mps
BACKEND=onnxruntime          # opencv, onnxruntime, openvino  
MODE=balanced                # performance, lightweight, balanced
CONFIDENCE_THRESHOLD=0.3     # Minimum confidence for keypoint detection

# Model Storage
MODEL_DIR=/app/models

# Redis Configuration (for background jobs)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
RQ_QUEUE_NAME=default

# Application Configuration
HOST=0.0.0.0
PORT=9090
DEBUG=False

# Logging
LOG_LEVEL=INFO
